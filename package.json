{
  "name": "snowshoe-api",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "scripts": {
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "studio": "pnpify prisma studio",
    "db:push": "pnpify prisma db push",
    "db:generate": "pnpify prisma generate",
    "proto:generate": "pnpify grpc_tools_node_protoc -I ./protobuf --js_out=import_style=commonjs,binary:./src/generated/proto --grpc_out=grpc_js:./src/generated/proto --ts_out=service=grpc-node,mode=grpc-js:./src/generated/proto ./protobuf/*.proto",
    "generate": "run-p db:generate proto:generate",
    "clean": "rimraf ./dist",
    "start": "node ./dist/index.js",
    "build": "run-s clean && tsc",
    "dev": "nodemon ./src/index.ts"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.5",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@yarnpkg/pnpify": "^3.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "grpc-tools": "^1.11.2",
    "nodemon": "^2.0.15",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1",
    "prisma": "3.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "ts-protoc-gen": "^0.15.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "yarn-run-all": "^3.1.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.5.7",
    "@grpc/proto-loader": "^0.6.9",
    "@prisma/client": "3.10.0",
    "google-protobuf": "^3.19.4"
  }
}
